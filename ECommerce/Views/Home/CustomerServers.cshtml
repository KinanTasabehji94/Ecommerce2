@model IEnumerable<ECommerce.Models.AspNetUserClaims>

@{
    ViewData["Title"] = "CustomerServers";
}

<h2>خدمة العملاء</h2>

<p>
    <a asp-area="Identity" asp-page="/Account/RegisterAdmin">اضافة موظف</a>
</p>
<table class="table table-bordered table-condensed table-striped">
    <thead>
        <tr>

            <th style="text-align:center">
               اسم الموظف
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.User.FullName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-UserId="@item.UserId">Edit Roles</a> 
                @*<a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>*@
            </td>
        </tr>
}
    </tbody>
</table>
